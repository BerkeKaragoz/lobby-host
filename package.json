{
   "name": "lobby-host-monorepo",
   "version": "0.0.0",
   "private": true,
   "workspaces": [
      "apps/*",
      "packages/*"
   ],
   "scripts": {
      "build": "turbo run build",
      "dev": "turbo run dev --parallel",
      "dev:todo": "cd ./apps/todo-lobby-host && pnpm dev",
      "lint": "turbo run lint",
      "format": "prettier --write \"**/*.{ts,tsx,md}\""
   },
   "devDependencies": {
      "prettier": "latest",
      "turbo": "latest"
   },
   "engines": {
      "npm": ">=7.0.0",
      "node": ">=14.0.0"
   },
   "packageManager": "pnpm@7.0.0-rc.9"
}
